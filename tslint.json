{
  "extends": "tslint-config-standard",
  "rules": {
    "semicolon": [true, "always"],
    "no-empty": false,
    "align": [false, "parameters", "statements"],
    "forin": true,
    "ter-indent": false,
    "quotemark": [true, "single"],
    "no-trailing-whitespace": true,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never"
      }
    ]
  }
}
